{
  "name": "NyanRPC",
  "version": "2.0.1",
  "description": "Simple Discord RPC Client ~nya",
  "author": "Shiraori <example@gmail.com>",
  "homepage": "https://github.com",
  "build": {
    "appId": "NyanRPC",
    "asar": false,
    "asarUnpack": [
      "**/*"
    ],
    "productName": "NyanRPC",
    "copyright": "Copyright Â© 2022 Shiraori#1782 & PinkDuwc._#3443",
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg",
      "icon": "src/Assets/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/Assets/icon.ico",
      "legalTrademarks": "GitHub"
    },
    "nsis": {
      "installerIcon": "src/Assets/icon.ico",
      "uninstallerIcon": "src/Assets/icon.ico",
      "uninstallDisplayName": "${productName} ${version}",
      "license": "license.txt",
      "multiLanguageInstaller": false,
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "menuCategory": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "main": "index.cjs",
  "type": "module",
  "scripts": {
    "test": "npm i && npm run download && npm run build && pause",
    "start": "electron index.cjs",
    "download": "npm i -g electron electron-builder",
    "buildPortable": "electron-packager . nyan --overwrite --platform=win32 --arch=x64 --electronVersion=20.0.0  --icon=src/Assets/icon.ico --prune=true --out=release-builds --version-string.CompanyName=Aiko  --version-string.FileDescription=A Discord RPC product  --version-string.ProductName=\"NyanRPC\"",
    "build:win": "electron-builder -w --x64",
    "build:mac": "electron-builder -m --x64",
    "build:linux": "electron-builder -l --x64"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "ascii-table": "^0.0.9",
    "body-parser": "^1.20.0",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "discord-rpc": "^4.0.1",
    "discord.js-selfbot-v13": "^2.4.10",
    "express": "^4.18.1",
    "path": "^0.12.7",
    "socket.io": "^4.5.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "electron": "^20.0.0",
    "electron-builder": "^23.3.3"
  }
}
